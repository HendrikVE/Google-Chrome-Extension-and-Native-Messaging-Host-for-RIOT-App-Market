{
    "key": "",
    "applications": {
        "gecko": {
            "id": "rapstore.extension@fu-berlin.de"
        }
    },
    "name": "RAPstore",
    "version": "1.0.7",
    "manifest_version": 2,
    "description": "Download your custom RIOT OS, enriched with applications you choose.",

    "icons": {
        "128": "icon-128.png"
    },

    "background": {
        "scripts": ["background_script.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "*://localhost/*",
            "*://141.22.28.91/*",
            "*://rapstore.cladmi.eu/*"
        ],
        "js": ["content_script.js"],
        "all_frames": true,
        "run_at": "document_end"
    }],

    "permissions": [
        "nativeMessaging"
    ]
}
